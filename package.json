{
    "name": "color-me-error",
    "displayName": "Color Me Error",
    "description": "Highlights TODO and FIXME (configurable) as errors.",
    "icon": "ICON.png",
    "version": "1.0.3",
    "publisher": "medo64",
    "galleryBanner": {
        "color": "#373E6D",
        "theme": "dark"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/medo64/color-me-error/issues/",
        "email": "jmedved@jmedved.com"
    },
    "homepage": "https://medo64.com/color-me-error/",
    "repository": {
        "url": "https://github.com/medo64/color-me-error/"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "todo",
        "fixme",
        "color"
    ],
    "engines": {
        "vscode": "^1.31.0"
    },
    "main": "./out/extension",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "configuration": [
            {
                "type": "object",
                "title": "Color Me Error",
                "properties": {
                    "colorMeError.words": {
                        "description": "Comma-separated list of words that will be colored as error.",
                        "scope": "language-overridable",
                        "type": "array",
                        "minItems": 1,
                        "items": {
                            "type": "string",
                            "minLength": 1
                        },
                        "default": [
                            "TODO",
                            "FIXME"
                        ]
                    }
                }
            }
        ]
    },
    "scripts": {},
    "devDependencies": {
        "eslint": "^8.8.0",
        "vsce": "^2.6.7",
        "vscode-test": "^1.6.1"
    }
}
